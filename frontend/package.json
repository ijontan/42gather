    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.3",
    "tailwindcss-labeled-groups": "^0.0.2",
    "typescript": "^5.0.0",
    "vite": "^4.5.0"
  },
  "type": "module",
